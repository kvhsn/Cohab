{
  "name": "@cohab/api",
  "version": "1.0.0",
  "scripts": {
    "start": "tsx watch --include src --include ../shared/src --env-file=.env src/index.ts",
    "build": "tsc",
    "lint": "eslint src && pnpm run prisma:validate",
    "format": "prettier --write src",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:db:seed": "prisma db seed",
    "prisma:validate": "prisma validate",
    "prisma:db:push": "prisma db push",
    "prisma:db:reset": "prisma migrate reset",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@better-auth/expo": "^1.4.19",
    "@cohab/shared": "workspace:*",
    "@hono/node-server": "^1.13.7",
    "@hono/zod-validator": "^0.7.6",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "better-auth": "^1.4.19",
    "dotenv": "^17.2.4",
    "eslint-config-prettier": "^10.1.8",
    "hono": "^4.6.14",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@prisma/client-runtime-utils": "^7.4.1",
    "@types/node": "^20.11.17",
    "@types/pg": "^8.16.0",
    "eslint": "^9.25.0",
    "prettier": "^3.8.1",
    "prisma": "^7.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
